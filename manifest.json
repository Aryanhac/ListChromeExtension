{
    "name": "Tasks List",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Creates A List of Tasks",
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "activeTab",
        "storage"
    ]
}